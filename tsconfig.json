{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client", "jest", "node"],
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "paths": {
        "@app/*": ["./bookWrench/*"],
        "@pages/*": ["./bookWrench/pages/*"],
        "@widgets/*":["./bookWrench/widgets/*"],
        "@routes":["./bookWrench/routes/index"],
        "@utils/*":["./bookWrench/utils/*"],
        "@common-utils/*":["./common/utils/*"],
        "@globals":["./common/utils/globals"],
        "@configs":["./bookWrench/configs/index"],
        "@dynamicConfig":["./bookWrench/configs/config-local"],
        "@storage":["./bookWrench/storage/index"],
        "@API":["./bookWrench/Agent/index"],
        "@ExposedPath":["./bookWrench/routes/ExposedPath"],
        "@common/*":["./common/*"],
        "@button":["./common/elements/Button"],
      },
      "typeRoots": ["./typings/", "./node_modules/@types/"]
    },
    "include": ["./common","./bookWrench", "./typings", "tailwind.config.js"],
    "exclude": ["node_modules"],
    "checkJs": true /* Report errors in .js files. Works in tandem with allowJs. */,
  }